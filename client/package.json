    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.109",
    "@mui/material": "^5.10.15",
    "@reduxjs/toolkit": "^1.9.1",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.2.0",
    "change-case": "^4.1.2",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.6.12",
